{
  "name": "lcs_country",
  "version": "1.0.0",
  "description": "A library providing country data, including region codes, capital cities, time zones, and currencies.",
  "main": "dist/lc.min.js",
  "module": "src/lc.js",
  "files": [
    "dist/lc.min.js",
    "dist/lcs_country.min.css"
  ],
  "scripts": {
    "build": "node obf.js && jsdoc -c jsdoc.json && npm run minify-css",
    "release": "npm run build && standard-version && git push --follow-tags && npm publish",
    "minify-css": "npx cleancss -o dist/lcs_country.min.css src/lcs_country.css",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lcsnigeria/lcs_country.git"
  },
  "keywords": [
    "country data",
    "continent codes",
    "country capitals",
    "time zones",
    "currency",
    "npm library",
    "LCS"
  ],
  "author": "jcfuniverse",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lcsnigeria/lcs_country/issues"
  },
  "homepage": "https://github.com/lcsnigeria/lcs_country#readme",
  "devDependencies": {
    "@node-minify/clean-css": "^8.0.6",
    "@node-minify/core": "^8.0.6",
    "clean-css-cli": "^5.6.3",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.0.0",
    "jsdoc": "^4.0.4",
    "standard-version": "^9.5.0"
  }
}
